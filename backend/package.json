{
  "name": "consultia-backend",
  "version": "1.0.0",
  "description": "ConsultIA Backend - Multi-tenant AI Agent Creation System",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "cdk:synth": "cd infra && cdk synth",
    "cdk:deploy": "cd infra && cdk deploy --all",
    "cdk:destroy": "cd infra && cdk destroy --all",
    "db:migrate": "node scripts/run-migrations.js",
    "db:seed": "node scripts/seed-data.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts,.js",
    "format": "prettier --write \"**/*.{ts,js,json,md}\""
  },
  "dependencies": {
    "aws-sdk": "^2.1691.0",
    "pg": "^8.12.0",
    "pg-pool": "^3.6.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/node": "^20.19.32",
    "@types/pg": "^8.11.6",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "aws-cdk": "^2.147.3",
    "aws-cdk-lib": "^2.147.3",
    "constructs": "^10.3.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
